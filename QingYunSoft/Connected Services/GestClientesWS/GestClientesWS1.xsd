      <xs:element minOccurs="0" maxOccurs="unbounded" name="reclamos" nillable="true" type="tns:reclamo" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="documentoCredito">
    <xs:sequence>
      <xs:element minOccurs="0" name="activo" type="xs:boolean" />
      <xs:element name="anulado" type="xs:boolean" />
